<?xml version="1.0"?>
<launch>
    <!--- laser_scan_matcher package base on laser scanner data -->
    <node pkg="laser_scan_matcher" type="laser_scan_matcher_node" name="laser_scan_matcher" respawn="false" output="screen">
        <param name="fixed_frame" value="map"/>
        <param name="base_frame" value="base_link"/>
        <param name="publish_tf" value="false"/>
        <!--- Odometery model parameters  -->
        <param name="use_imu" value="false"/>
        <param name="use_odom" value="false"/>
    </node>
</launch>
